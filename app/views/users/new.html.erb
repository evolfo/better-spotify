<h1>Create a new account:</h1>
<%= form_for @user do |f| %>

 Username
 <br>
 <%= f.text_field :username %>
 <br>
 <br>
 Email
 <br>
 <%= f.email_field :email %>
 <br>
 <br>
 Password
 <br>
 <%= f.password_field :password %>
 <br>
 <br>
 Password Confirmation
 <br>
 <%= f.password_field :password_confirmation %>
 <br>
 <br>

 <%= f.submit "Create Account"%>

<% end %>
